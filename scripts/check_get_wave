from t8_client.api import T8ApiClient
import os
from dotenv import load_dotenv

load_dotenv()

client = T8ApiClient(
    host=os.getenv("T8_HOST"),
    user=os.getenv("T8_USER"),
    password=os.getenv("T8_PASSWORD")
)

timestamp = "2019-04-10T14:48:44" #1554907724

wave_data = client.get_wave("LP_Turbine", "MAD31CY005", "AM1", timestamp)


